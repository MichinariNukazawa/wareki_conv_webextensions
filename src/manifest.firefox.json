{
  "manifest_version": 2,
  "name": "daisy WarekiConv",
  "description": "In page convert with Japanese calendar to Western calendar.",
  "homepage_url": "https://github.com/MichinariNukazawa/wareki_conv_webextension/tree/master/",
  "version": "1.2.0",
  "icons": {
    "48": "icons/icon.png",
    "96": "icons/icon@2x.png"
  },

  "content_scripts": [
    {
      "matches": ["https://*/*", "http://*/*"],
      "js": ["./kanNum.js", "./warekiEngine.js", "./substitute.js"]
    }
  ]
}
